{
  "name": "static-site-skeleton",
  "description": "Static Site Skeleton",
  "version": "1.0.0",
  "author": "Reece Lucas <reecelucas@sky.com>",
  "repository": "https://github.com/reecelucas/gatsby-site-skeleton",
  "license": "MIT",
  "main": "n/a",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "yarn format && gatsby build && gatsby serve",
    "build:server": "gatsby build",
    "develop": "yarn format && gatsby develop",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "lint": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}' && stylelint 'src/**/*.scss'",
    "test": "jest --no-cache",
    "test:coverage": "jest --coverage --no-cache",
    "precommit": "yarn lint && yarn format"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "IE 10"
  ],
  "dependencies": {
    "@types/classnames": "^2.2.3",
    "@types/fontfaceobserver": "^0.0.6",
    "@types/graphql": "^0.12.4",
    "@types/jest": "^22.1.2",
    "@types/node": "9.4.6",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^16.0.4",
    "@types/react-helmet": "^5.0.3",
    "autoprefixer": "^8.0.0",
    "babel-jest": "^22.2.2",
    "classnames": "^2.2.5",
    "css-mqpacker": "^6.0.1",
    "fontfaceobserver": "^2.0.13",
    "gatsby": "^1.9.221",
    "gatsby-link": "^1.6.38",
    "gatsby-plugin-manifest": "^1.0.8",
    "gatsby-plugin-offline": "^1.0.9",
    "gatsby-plugin-react-helmet": "^2.0.6",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sass": "^1.0.12",
    "gatsby-plugin-typescript": "^1.4.8",
    "glob": "^7.1.2",
    "jest": "^22.2.2",
    "optimize-js-plugin": "^0.0.4",
    "path": "^0.12.7",
    "postcss-assets-webpack-plugin": "^1.1.0",
    "postcss-discard-duplicates": "^2.1.0",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "react": "^16.2.0",
    "stylelint": "^9.1.1",
    "stylelint-scss": "^2.1.0",
    "ts-jest": "^22.0.4",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.8.1"
  },
  "devDependencies": {
    "ajv": "^6.2.0",
    "babel-eslint": "8.0.0",
    "husky": "^0.14.3",
    "prettier": "1.11.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "sass-mq": "^4.0.2",
    "tslint": "^5.9.1"
  }
}
